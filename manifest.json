{
  "manifest_version": 3,
  "name": "Apps Shortcut",
  "version": "0.2",
  "description": "Keyboard Shortcut to Chrome Apps",
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "new-apps-tab": {
      "suggested_key": {
        "default": "Alt+Shift+A"
      },
      "description": "Open Chrome Apps in a new Tab"
    }
  }
}